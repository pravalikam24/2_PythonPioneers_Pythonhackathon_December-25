import pandas as pd
import numpy as np

df1 = pd.read_csv("dataset1.csv") # 260K rows
df2 = pd.read_csv("dataset2.csv") # 15K rows
df3 = pd.read_csv("dataset3.csv") # 15K rows

def profile_dataframe(df, name):
profile = pd.DataFrame({
"column": df.columns,
"dtype": df.dtypes.astype(str),
"missing_pct": df.isna().mean().round(3),
"n_unique": df.nunique(dropna=True)
})
profile["dataset"] = name
return profile
